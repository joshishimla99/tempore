<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

	<!-- Servlets -->
	<servlet>
		<servlet-name>ProjectServices</servlet-name>
		<servlet-class>ar.fi.uba.tempore.gwt.server.ProjectServicesImpl</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UserServices</servlet-name>
		<servlet-class>ar.fi.uba.tempore.gwt.server.UserServicesImpl</servlet-class>
	</servlet>




	<servlet-mapping>
		<servlet-name>ProjectServices</servlet-name>
		<url-pattern>/tempore/ProjectServices</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UserServices</servlet-name>
		<url-pattern>/tempore/UserServices</url-pattern>
	</servlet-mapping>


	<!-- Default page to serve -->
	<welcome-file-list>
		<welcome-file>Tempore.html</welcome-file>
	</welcome-file-list>

</web-app>
