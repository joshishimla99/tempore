<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='tempore'>
	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='com.google.gwt.user.User' />
	
	<inherits name='ar.fi.uba.tempore.DTO' />
	<inherits name='ar.fi.uba.temporeutils.TemporeListGrid' />
	<inherits name='ar.fi.uba.temporeutils.Observer' />
	<inherits name='ar.fi.uba.temporeutils.Image' />

	<!-- External library -->	
	<inherits name="gwtupload.GWTUpload"/>
	<stylesheet src="Upload.css"/>

	<!-- Inherit the default GWT style sheet. You can change -->
	<!-- the theme of your GWT application by uncommenting -->
	<!-- any one of the following lines. -->
	<inherits name='com.google.gwt.user.theme.standard.Standard' />
	<!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
	<!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/> -->

	<!-- Other module inherits -->

	<!-- Inherit the SmartGWT Toolkit library configuration. -->
	<inherits name='com.smartgwt.SmartGwt' />

	<!-- Specify the app entry point class. -->
	<entry-point class='ar.fi.uba.tempore.gwt.client.Tempore' />
	<!-- <entry-point class='ar.tempore.client.Principal'/> -->

	<!-- Specify the paths for translatable code -->
	<source path='client' />
	<source path='DTO' />
	<source path='TemporeListGrid' />
</module>
